import math


print("Введите a");
a=input();

print("ВВедите b");
b=input();

print("Введите c");
c=input();

d=b*b-4*a*c

if(d>0):
    print("x1: ");
    print((-b+math.sqrt(d))/(2*a));
    print("x2: ");
    print((-b-math.sqrt(d))/(2*a));
elif(d==0):
    print("x1 and x2: ");
    print((-d)/(2*a));
else:
    print("Не могу посчитать дискриминант меньше 0");
